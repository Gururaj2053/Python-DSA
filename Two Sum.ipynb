   "source": [
    "def twosum(nums :list[int], target: int) -> list[int]:\n",
    "    d = {}\n",
    "    for i, j in enumerate(nums):\n",
    "        r = target - j\n",
    "        if r in d: return [d[r], i]\n",
    "        d[j] = i\n",
    "        print(d)"
   ]
